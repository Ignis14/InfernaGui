-- Scripts:

local function GFYGB_fake_script() -- PlayerBtn.textColourChange 
	local script = Instance.new('LocalScript', PlayerBtn)

	script.Parent.MouseEnter:Connect(function()
		script.Parent.TextColor3 = Color3.fromRGB(85, 255, 255)
	end)
	
	script.Parent.MouseLeave:Connect(function()
		script.Parent.TextColor3 = Color3.fromRGB(255, 255, 255)
	end)
end
coroutine.wrap(GFYGB_fake_script)()
local function HFZIWY_fake_script() -- PlayerBtn.LocalScript 
	local script = Instance.new('LocalScript', PlayerBtn)

	script.Parent.MouseButton1Down:Connect(function()
