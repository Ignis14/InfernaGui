local function RBIOQXW_fake_script() -- receivedBtn.LocalScript 
	local script = Instance.new('LocalScript', receivedBtn)

	local toggle = false
	
	script.Parent.MouseButton1Down:Connect(function()
		local starterGui = game:GetService("StarterGui")
		if toggle == false then
			starterGui:SetCore("SendNotification",{
				Title = "Script Executed ✔",
				Text = "You will no longer see what you received from buying"
			})
			toggle = true
			
			game.Players.LocalPlayer.PlayerGui.ItemGained.Enabled = false
			
		else
			starterGui:SetCore("SendNotification",{
				Title = "Script Executed",
				Text = "You will now see what you received from buying"
			})
			toggle = false
			
			game.Players.LocalPlayer.PlayerGui.ItemGained.Enabled = true
			
		end
		
	end)
end
coroutine.wrap(RBIOQXW_fake_script)()
